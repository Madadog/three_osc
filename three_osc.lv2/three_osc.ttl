@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix props: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<https://github.com/Madadog/three_osc>
        a lv2:Plugin ,
                lv2:InstrumentPlugin ;
        lv2:project <https://github.com/Madadog/three_osc> ;

        doap:name "Three Osc" ;
        doap:license <https://www.gnu.org/licenses/gpl-3.0.html> ;
        lv2:requiredFeature urid:map ;
        lv2:optionalFeature lv2:hardRTCapable ;

        lv2:port [
                a lv2:InputPort ,
                    atom:AtomPort ;
                atom:bufferType atom:Sequence ;
                atom:supports midi:MidiEvent ;
                lv2:designation lv2:control ;
                lv2:index 0 ;
                lv2:symbol "midi" ;
                lv2:name "Midi In"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 1 ;
                lv2:symbol "out_l" ;
                lv2:name "Output L"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 2 ;
                lv2:symbol "out_r" ;
                lv2:name "Output R"
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "osc1_amp" ;
                lv2:name "Osc1 Amplitude" ;
                lv2:default 100.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 100.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "osc1_semitone" ;
                lv2:name "Osc1 Pitch" ;
                lv2:default 0.0 ;
                lv2:minimum -24.0 ;
                lv2:maximum 24.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "osc1_exponent" ;
                lv2:name "Osc1 Freq. Exponent" ;
                lv2:default 0 ;
                lv2:minimum -16 ;
                lv2:maximum 16 ;
                lv2:portProperty lv2:integer ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "osc1_wave" ;
                lv2:name "Osc1 Wave" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 4 ;
                lv2:portProperty lv2:integer ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "osc1_mod" ;
                lv2:name "Osc1 Mod" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 4 ;
                lv2:portProperty lv2:integer ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "osc1_voices" ;
                lv2:name "Osc1 Voices" ;
                lv2:default 1 ;
                lv2:minimum 1 ;
                lv2:maximum 128 ;
                lv2:portProperty lv2:integer ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "osc1_super_detune" ;
                lv2:name "Osc1 Super Detune" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 100.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "osc1_phase" ;
                lv2:name "Osc1 Phase" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 360.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "osc1_phase_rand" ;
                lv2:name "Osc1 Phase Rand." ;
                lv2:default 100.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 100.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "output_gain" ;
                lv2:name "Output Gain" ;
                lv2:default 0.0 ;
                lv2:minimum -90.0 ;
                lv2:maximum 12.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "global_pitch" ;
                lv2:name "Global Pitch" ;
                lv2:default 0.0 ;
                lv2:minimum -24.0 ;
                lv2:maximum 24.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "vol_attack" ;
                lv2:name "Vol Attack" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 20.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 15 ;
                lv2:symbol "vol_decay" ;
                lv2:name "Vol Decay" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 20.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 16 ;
                lv2:symbol "vol_sustain" ;
                lv2:name "Vol Sustain" ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ] , [
                a lv2:InputPort ,
                    lv2:ControlPort ;
                lv2:index 17 ;
                lv2:symbol "vol_release" ;
                lv2:name "Vol Release" ;
                lv2:default 0.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 20.0 ;
        ] .