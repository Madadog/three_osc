@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix props: <http://lv2plug.in/ns/ext/port-props#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix urid: <http://lv2plug.in/ns/ext/urid#> .

<https://github.com/Madadog/three_osc>
        a lv2:Plugin ,
                lv2:ReverbPlugin ;
        lv2:project <https://github.com/Madadog/three_osc> ;

        doap:name "Three Osc" ;
        doap:license <https://www.gnu.org/licenses/gpl-3.0.html> ;
        lv2:requiredFeature urid:map ;
        lv2:optionalFeature lv2:hardRTCapable ;

        lv2:port [
		a lv2:InputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		lv2:designation lv2:control ;
		lv2:index 0 ;
		lv2:symbol "midi" ;
		lv2:name "Midi In"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 1 ;
                lv2:symbol "out_l" ;
                lv2:name "Out Left"
        ] , [
                a lv2:AudioPort ,
                        lv2:OutputPort ;
                lv2:index 2 ;
                lv2:symbol "out_r" ;
                lv2:name "Out Right"
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 3 ;
                lv2:symbol "gain" ;
                lv2:name "Output Gain" ,
                        "收益"@ch ,
                        "Gewinn"@de ,
                        "Output Gain"@en-gb ,
                        "Aumento"@es ,
                        "Gain"@fr ,
                        "Guadagno"@it ,
                        "ゲイン"@jp ,
                        "Увеличение"@ru ;
                lv2:default 0.0 ;
                lv2:minimum -90.0 ;
                lv2:maximum 24.0 ;
                units:unit units:db ;
                props:displayPriority 0 ;
                lv2:scalePoint [
                        rdfs:label "+5" ;
                        rdf:value 5.0
                ] , [
                        rdfs:label "0" ;
                        rdf:value 0.0
                ] , [
                        rdfs:label "-5" ;
                        rdf:value -5.0
                ] , [
                        rdfs:label "-10" ;
                        rdf:value -10.0
                ]
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "wet_dry_ratio" ;
                lv2:name "Wet/Dry Ratio" ;
                lv2:default 0.15 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "wet_dry_ratio" ;
                lv2:name "Wet/Dry Ratio" ;
                lv2:default 0.15 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "feedback" ;
                lv2:name "Feedback" ;
                lv2:default 0.97 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                units:unit units:pc ;
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "damp" ;
                lv2:name "Damping" ;
                lv2:default 0.5465 ;
                lv2:minimum 0.0 ;
                lv2:maximum 1.0 ;
                units:unit units:hz ;
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "sample_rate" ;
                lv2:name "Sample Rate" ;
                lv2:default 44100 ;
                lv2:minimum 100.0 ;
                lv2:maximum 132300 ;
                units:unit units:hz ;
        ] , [
                a lv2:InputPort ,
                        lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "pitch_mod" ;
                lv2:name "Pitch Mod" ;
                lv2:default 1.0 ;
                lv2:minimum 0.0 ;
                lv2:maximum 10.0 ;
        ] .
